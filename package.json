{
	"type": "module",
	"version": "0.0.1",
	"name": "astro-advanced-poc",
	"scripts": {
		"dev": "astro dev --port 3000",
		"dev:https": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"test": "vitest --run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit",
		"check": "astro check --minimumSeverity=warning --minimumFailingSeverity=warning",
		"verify": "pnpm test && pnpm typecheck && pnpm check",
		"generate:graphql": "graphql-codegen --config codegen.ts",
		"generate:locales": "sh pull-locale-json.sh && tsx ./src/i18n/import.ts"
	},
	"engines": {
		"node": ">=18.14.1"
	},
	"dependencies": {
		"@astrojs/node": "6.0.1",
		"@astrojs/partytown": "2.0.0",
		"@astrojs/svelte": "4.0.2",
		"@astrojs/tailwind": "5.0.0",
		"@urql/core": "4.1.3",
		"astro": "3.1.2",
		"astro-compressor": "0.4.0",
		"google-auth-library": "^8.7.0",
		"google-oauth-gsi": "4.0.0",
		"graphql-tag": "^2.12.6",
		"js-cookie": "^3.0.5",
		"jwt-decode": "^3.1.2",
		"lodash-es": "^4.17.21",
		"svelte": "4.2.1",
		"tailwindcss": "3.3.3",
		"typesafe-i18n": "^5.24.3",
		"vite": "^4.4.9",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@astrojs/check": "0.2.0",
		"@astrojs/ts-plugin": "1.1.3",
		"@babel/core": "^7.22.20",
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-document-nodes": "4.0.1",
		"@graphql-codegen/typescript-operations": "4.0.1",
		"@sveltejs/vite-plugin-svelte": "2.4.6",
		"@types/js-cookie": "^3.0.3",
		"@types/lodash-es": "^4.17.7",
		"astro-compress": "2.0.15",
		"autoprefixer": "10.4.16",
		"graphql": "16.8.1",
		"node-fetch": "3.3.2",
		"postcss": "^8.4.30",
		"prettier": "3.0.3",
		"prettier-plugin-astro": "0.12.0",
		"prettier-plugin-svelte": "3.0.3",
		"tsx": "3.12.10",
		"typescript": "5.2.2",
		"vite-plugin-mkcert": "1.16.0",
		"vitest": "^0.31.4"
	}
}
